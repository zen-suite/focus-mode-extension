{
  "manifest_version": 3,
  "name": "Zen mode",
  "description": "An extension for blocking websites to keep you in zen.",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "permissions": ["activeTab"]
}
